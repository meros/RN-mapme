{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "react/default-props-match-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "no-extra-parens": 0,
    "no-use-before-define": 0,
    "no-mixed-operators": 0,
    "react/sort-comp": 0,
    "import/no-named-as-default": 0
  }
}
